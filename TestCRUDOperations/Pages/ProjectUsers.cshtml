@page
@model TestCRUDOperations.Web.Pages.ProjectUsersModel

@{
    ViewData["Title"] = "ProjectUsers";
}

<h2 class="text-center">Users of project @Model.Project.Name</h2>

<table class="table table-striped table-bordered">
    <thead>
        <tr>
                <th>
                    First name
                </th>
                <th>
                    Last name
                </th>
                <th>
                    Age
                </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Users) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Age)
            </td>            
        </tr>
}
    </tbody>
</table>
